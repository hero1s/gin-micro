                "age": {
                    "description": "年龄",
                    "type": "integer"
                },
                "name": {
                    "description": "名字",
                    "type": "string"
                }
            }
        },
        "response.JsonObject": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "状态码",
                    "type": "string"
                },
                "content": {
                    "description": "数据",
